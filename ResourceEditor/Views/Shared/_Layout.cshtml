<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ResX Editor</title>
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/Kendo")

    @Scripts.Render("~/bundles/modernizr")

    @Styles.Render("~/Content/site")

    @*<link rel="stylesheet" href="http://kendo.cdn.telerik.com/2018.3.911/styles/kendo.common.min.css">*@
    @*<link rel="stylesheet" href="http://cdn.kendostatic.com/2012.1.322/styles/kendo.default.min.css"/>*@

    <script type="text/javascript">
        var urlControlJsonMethod = "@Url.Action("JsonResolver", "Home")";
    </script>

</head>
<body>
    <header>
        <ul>
            <li>@Html.ActionLink("Add Line Resourcel", "AddLineResource", "Home")</li>
            <li>@Html.ActionLink("Main Table", "MainTableResource", "Home")      </li>
            <li>@Html.ActionLink("Json Resolver", "JsonResolver", "Home", null, new { id = "" })        </li>
            <li><button type="submit" id="buttonJson">Generate Json</button></li>

        </ul>

    </header>
    <main>
        @RenderBody()
    </main>

    <footer>      </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Kendo")
    @Scripts.Render("~/bundles/main")

    @*<script src="http://kendo.cdn.telerik.com/2018.3.911/js/kendo.all.min.js"></script>*@

    <div id="example">
        <div id="grid"></div>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
