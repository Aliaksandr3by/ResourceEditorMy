<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ResX Editor</title>
    @Styles.Render("~/Content/css")
    @*@Styles.Render("~/Content/Kendocommon")*@
    @Styles.Render("~/Content/Kendotheme")
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/products")


    <link rel="stylesheet" href="http://kendo.cdn.telerik.com/2018.3.911/styles/kendo.common.min.css">
    @*<link href="http://cdn.kendostatic.com/2012.1.322/styles/kendo.default.min.css" rel="stylesheet" />*@

</head>
<body>
    <header>
        @Html.ActionLink("Add Line Resourcel", "AddLineResource", "Home")
        @Html.ActionLink("Main Table", "MainTableResource", "Home")
    </header>
    <main>
        @RenderBody()
    </main>

    <footer>      </footer>

    <div id="example">
        <div id="grid"></div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @*@Scripts.Render("~/bundles/Kendo")*@
    @Scripts.Render("~/bundles/main")

    <script src="http://kendo.cdn.telerik.com/2018.3.911/js/kendo.all.min.js"></script>


    @RenderSection("scripts", required: false)
</body>
</html>
