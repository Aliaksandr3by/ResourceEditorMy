<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script>
        const urlControlActionDelete = "@Url.Action("Delete", "Home")";
        const urlControlActionUpdate = "@Url.Action("Update", "Home")";
        const urlControlActionDataProtect = "@Url.Action("DataProtect", "Home")";
        const urlControlSwitchLanguage = "@Url.Action("SwitchLanguage", "Home")";
        const urlControlUploadFile = "@Url.Action("UploadFile", "Home")";
        const urlControlGetFile = "@Url.Action("GetFile", "Home")";
        const urlControlSelectCountry = "@Url.Action("SelectCountry", "Home")";
        const urlControlRead = "@Url.Action("Read", "Home")";
        const urlControlLogFile = "@Url.Action("LogFile", "Home")";
    </script>

    <meta http-equiv="Cache-Control" content="no-cache">

    <title>@ViewBag.Title - ResX Editor</title>

    @RenderSection("scriptsHead", required: true)
    <script src="~/dist/Link.js"></script>
</head>

<body>
    <noscript>
        You need to enable JavaScript to run this app.
    </noscript>
    <div class="container-grid">
        <header class="container-grid-item grid-item-header" role="navigation">
            @Html.Partial("NavigateBar")
        </header>
        <main class="container-grid-item grid-item-main" role="main">
            @RenderBody()
        </main>
        <footer class="container-grid-item grid-item-footer" role="status">
            @DateTime.Now
        </footer>
    </div>

    <div id="panelNavigation"></div>

    <div id="IE11"></div>

    @RenderSection("scriptsFooter", required: true)

</body>

</html>