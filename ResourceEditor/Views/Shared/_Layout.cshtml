<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ResX Editor</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Kendo")
    @Scripts.Render("~/bundles/modernizr")

    @*<link rel="stylesheet" href="http://kendo.cdn.telerik.com/2018.3.911/styles/kendo.common.min.css">*@
    @*<script src="http://kendo.cdn.telerik.com/2018.3.911/js/kendo.all.min.js"></script>*@
</head>
<body>
    <header>
        @Html.ActionLink("Add Line Resourcel", "AddLineResource", "Home")
        @Html.ActionLink("Main Table", "MainTableResource", "Home")
    </header>
    <main>
        @RenderBody()
    </main>

    <footer>    </footer>

    <div id="example">
        <div id="grid"></div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Kendo")
    @Scripts.Render("~/bundles/main")
    @RenderSection("scripts", required: false)
</body>
</html>
